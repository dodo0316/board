<script>
	document.getElementById('auto-search-input').addEventListener("keyup", makedelay(function(e){
		var code = (e.keyCode || e.which);
		if(code == 37 || code == 39 || code == 40 || code == 38 || code == 16 || code == 17 || code == 18 || code == 21) {
			return;
		}
		autoSearch(this.value);
		}, 500));
		this.value, code = null;
		
		function makedelay(callback, ms) {
		var timer = 0;

		try{
			return returnTime = function() {
				var context = this, args = arguments;
				clearTimeout(timer);
				timer = setTimeout(function () {
				callback.apply(context, args);
				}, ms || 0);
			};
		}
		finally{
			returnTime, timer = null;
		}
	}
</script>

