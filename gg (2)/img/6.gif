    <script>
    
	// 뉴스레터 구독
    function regNewsLetter(){
        newsletterInit();
        $.ajax({
			url: `get_user_data.php`,
			method: "POST",
			data:{
				'action' 		: 'newsletter'
			},
			async: true,
			beforeSend: function () {
				
			},
			success: function (res) {
				let data = JSON.parse(res)[0];
                let state = data.result;
                let email = data.email;
                let text = document.getElementById(`newletter-modal-text`);
                let regBtn = document.getElementById(`newletter-reg-btn`);
                let alertText = '';
                if(state == 'already'){
                    $('#newletter-modal').modal('show');
                    text.innerHTML = `<span class='bold'>${email}</span> 메일로 뉴스레터를 구독하고 있습니다.`;
                    regBtn.innerHTML = `구독취소`;
                    regBtn.setAttribute(`onclick`,`newsletterCancel()`);
                    regBtn.classList.remove(`btn-primary`);
                    regBtn.classList.add(`btn-secondary`);
                }else if(state == 'yet'){
                    $('#newletter-modal').modal('show');
                    text.innerHTML = `<span class='bold'>${email}</span> 메일로 뉴스레터를 구독할까요?`;
                    regBtn.innerHTML = `구독`;
                    regBtn.setAttribute(`onclick`,`newsletterOk()`);
                    regBtn.classList.add(`btn-primary`);
                    regBtn.classList.remove(`btn-secondary`);
                }else{
                    $('#newletter-modal').modal('show');
                    text.innerHTML = `현재 계정에 메일 정보가 존재하지 않습니다.<br/>이메일 변경 버튼을 클릭해 이메일 정보를 입력해주세요.`;
                }

				xhr: /*@cc_on false && @*/window.XMLHttpRequest ?
				function() { return new window.XMLHttpRequest(); } :
				function() {
					try { return new window.ActiveXObject("Microsoft.XMLHTTP"); }
					catch(e) {}
				}
				// setTimeout(autoSearch, 1000);
			},
			error: function(e) { 
				alert("잘못된 페이지로 접근하였습니다."); 
			}
		});
		
	}

    function newsletterInit(){
        let text = document.getElementById(`newletter-modal-text`);
        let form = document.getElementById(`email-form-div`);
        let finishBtn = document.getElementById(`newletter-change-finish-btn`);
        let changeBtn = document.getElementById(`newletter-change-btn`);
        let regBtn = document.getElementById(`newletter-reg-btn`);
        let cancelBtn = document.getElementById(`newletter-cancel-btn`);
        text.classList.remove(`display-none`);
        changeBtn.classList.remove(`display-none`);
        regBtn.classList.remove(`display-none`);

        form.classList.add(`display-none`);
        finishBtn.classList.add(`display-none`);
        cancelBtn.classList.add(`display-none`);
        
        
    }
    function emailChange(){
        let text = document.getElementById(`newletter-modal-text`);
        let form = document.getElementById(`email-form-div`);
        let finishBtn = document.getElementById(`newletter-change-finish-btn`);
        let changeBtn = document.getElementById(`newletter-change-btn`);
        let regBtn = document.getElementById(`newletter-reg-btn`);
        let cancelBtn = document.getElementById(`newletter-cancel-btn`);
       
        finishBtn.setAttribute('disabled','disabled');

        text.classList.add(`display-none`);
        changeBtn.classList.add(`display-none`);
        regBtn.classList.add(`display-none`);

        form.classList.remove(`display-none`);
        finishBtn.classList.remove(`display-none`);
        cancelBtn.classList.remove(`display-none`);
    }

    function emailChangeReg(){
        let email = document.getElementById(`new-email`).value;
        $.ajax({
			url: `get_user_data.php`,
			method: "POST",
			data:{
				'action' 		: 'emailChangeReg',
                'email'         : email
			},
			async: true,
			beforeSend: function () {
				
			},
			success: function (res) {
				regNewsLetter();
				xhr: /*@cc_on false && @*/window.XMLHttpRequest ?
				function() { return new window.XMLHttpRequest(); } :
				function() {
					try { return new window.ActiveXObject("Microsoft.XMLHTTP"); }
					catch(e) {}
				}
				// setTimeout(autoSearch, 1000);
			},
			error: function(e) { 
				alert("잘못된 페이지로 접근하였습니다."); 
			}
		});
        
    }

    function newsletterOk(){
        $.ajax({
			url: `get_user_data.php`,
			method: "POST",
			data:{
				'action' 		: 'newsletterOk'
			},
			async: true,
			beforeSend: function () {
				
			},
			success: function (res) {
                $('#newletter-modal').modal('hide');
				alertBox('뉴스레터 구독 신청이 완료되었습니다.', '', 2000);
				xhr: /*@cc_on false && @*/window.XMLHttpRequest ?
				function() { return new window.XMLHttpRequest(); } :
				function() {
					try { return new window.ActiveXObject("Microsoft.XMLHTTP"); }
					catch(e) {}
				}
				// setTimeout(autoSearch, 1000);
			},
			error: function(e) { 
				alert("잘못된 페이지로 접근하였습니다."); 
			}
		});
    }

    function newsletterCancel(){
        let Alert = confirm('더 이상 보드게임과 관련된 새로운 정보를 받아볼 수 없습니다.\n보드라이프 뉴스레터 구독을 취소할까요?');
        if(Alert){
            $.ajax({
                url: `get_user_data.php`,
                method: "POST",
                data:{
                    'action' 		: 'newsletterCancel'
                },
                async: true,
                beforeSend: function () {
                    
                },
                success: function (res) {
                    $('#newletter-modal').modal('hide');
                    alertBox('뉴스레터 구독이 취소되었습니다.', '', 2000);
                    xhr: /*@cc_on false && @*/window.XMLHttpRequest ?
                    function() { return new window.XMLHttpRequest(); } :
                    function() {
                        try { return new window.ActiveXObject("Microsoft.XMLHTTP"); }
                        catch(e) {}
                    }
                    // setTimeout(autoSearch, 1000);
                },
                error: function(e) { 
                    alert("잘못된 페이지로 접근하였습니다."); 
                }
            });
        }else{
            $('#newletter-modal').modal('hide');
        }
        
    }

    function inputEmailformat(obj){
        // console.log(obj.value);
        let finishBtn = document.getElementById(`newletter-change-finish-btn`);
        let regExp = /^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/;
        if (regExp.test(obj.value)) {
            //정규식 충족
            finishBtn.removeAttribute('disabled');
        }
    }

    let alertBoxClear = ''
    function alertBox(text, type, time=``){
        alertBoxStart(text, type);
        let ShowTime = time == '' ? 1000 : time;
        alertBoxClear = setTimeout(alertBoxEnd, ShowTime);
    }


    function alertBoxStart(text, type){
        let alertBox = document.getElementById('alert-box');
        let message = document.getElementById('alert-message');
        let bg = document.getElementById(`blank-bg`);
        if(type == 'error'){
            IdAddClass(`alert-box`,`error`);
        }
        bg.classList.remove('display-none');
        alertBox.classList.remove('display-none');
        message.innerHTML = text;
    }

    function alertBoxEnd(){
        let bg = document.getElementById(`blank-bg`);
        let alertBox = document.getElementById('alert-box');
        bg.classList.add('display-none');
        alertBox.classList.add('display-none');
        IdRemoveClass(`alert-box`,`error`);
        clearTimeout(alertBoxClear);
    }
</script>
